<%= form_for(@pet) do |f|%>
<%=f.label :name  %>
<%=f.text_field :name  %>
<%=f.label :address  %>
<%=f.text_field :address  %>
<%=f.label :spacies,  type: 'select', collection: %w[dog cat ferret parrot snake]%>
<%=f.text_field :spacies  %>
<%=f.label :found_on  %>
<%=f.date_field :found_on  %>
<%= f.submit  %>
<% end %>

<%= link_to  "back home", pets_path %>